<!DOCTYPE html>
<html>
    <head>
        {% include theme_path('partials/head.html') %}
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-50774673-1"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-50774673-1');
        </script>

    </head>
    <body>

        <div id="wrapper">
            {% include theme_path('partials/header.html') %}
            <!-- Site Content -->
            <div id="main">
                {% block content %}{% endblock %}
            </div>
            <!--Content End Here -->
            {% include theme_path('partials/footer.html') %}

        </div>
        {% include theme_path('partials/scripts.html') %}
    </body>
</html>